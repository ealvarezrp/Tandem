           FOR STABLE ACCESS
      END-EXEC.
      IF SQLCODE NOT = 0
         set ESQLERR to TRUE
         move 1000   to w-cc-compl-cd
         move 1      to w-cc-stop-opt
         DISPLAY PSS-NUM-COPIA " Err " SQLCODE
               " on UPDATE CREFISTR "
               " folio= " RFTR-NUMF-TRANSACCION
            upon HOME-TERM
      end-if.

 9000-INSERT-SREFISSA.
