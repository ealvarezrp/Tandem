             , RFTR_FECH_FIN_OPERACION    = :RFTR-FECH-FIN-OPERACION
             , RFTR_IMPO_INTERES_NOMINAL  = :RFTR-IMPO-INTERES-NOMINAL
             , RFTR_IMPO_INTERES_REAL     = :RFTR-IMPO-INTERES-REAL
