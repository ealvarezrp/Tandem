         WHERE RFTR_ANIO_MES         = :RFTR-ANIO-MES
           AND RFTR_NUM_CONTRATO     = :RFTR-NUM-CONTRATO
           AND RFTR_NUM_PRODUCTO     = :RFTR-NUM-PRODUCTO
           AND RFTR_CVE_TIPO_SALDO   = :RFTR-CVE-TIPO-SALDO
