ADD DEFINE =SEDOPAIS, FILE $DESA6.BADPEDTB.SEDOPAIS

PURGE $DESA.LALO.LONGF
inlprefix -
FUP /PRI 159, INLINE/
- SET TYPE E
- SET FORMAT 2
- SET EXT (90000, 32000)
- SET BLOCK 4096
- SET BUFFERED
- SET REC 1000
- SET MAXEXTENTS 50
- CREATE $DESA.LALO.LONGF
- EXIT

inlprefix -
SQLCI /CPU 0, INLINE/
- PURGEDATA $DESA.LALO.LONGF;
- OUT $DESA.LALO.LONGF;

- SET SESSION AUTOWORK OFF;
- SET LAYOUT LEFT_MARGIN 1;
- SET LAYOUT PAGE_LENGTH ALL;
- SET LAYOUT SPACE 1;
- SET STYLE HEADINGS ON;
- SET STYLE DATE_FORMAT "Y4M2D2";

- SELECT RFPE_NUM_CONTRATO
-       ,RFPE_NUM_PERSONA
-       ,RFPE_CVE_ROL_CONTRATO
-       ,RFPE_CVE_TIPO_PERSONA
-       ,RFPE_NOMB_APELLIDO_PATERNO
-       ,RFPE_NOMB_APELLIDO_MATERNO
-       ,RFPE_NOMB_PERSONA_FISICA
-       ,RFPE_NOMB_RAZON_SOCIAL
-       ,RFPE_CODX_RFC
-       ,RFPE_CODX_CURP
-       ,RFPE_PORC_DISTRIBUCION
-       ,RFPE_CODX_IMPRESION
-       ,RFPE_ANIO_MES
-       ,RFDO_TXT_CALLE_NUM
-       ,RFDO_TXT_COLONIA
-       ,RFDO_TXT_DELEGACION_MUNICIPIO
-       ,RFDO_CODX_POSTAL
-       ,RFDO_NUM_ESTADO
-       ,COPO_CENTRO_REPARTO
-       ,RFPE_NUM_OPCION_ENVIO
-       ,EDOP_DESC_ESTADO
-       ,RFPE_CVE_PAIS_NACIONALIDAD
- FROM =SREFISPE
-     ,=SREFISAC
-     ,=SCODPOST
-     ,=SREFISDO LEFT JOIN =SEDOPAIS ON
-                RFDO_NUM_ESTADO = EDOP_NUM_ESTADO
      
- WHERE RFPE_ANIO_MES            = RFAC_ANIO_MES
- AND   RFAC_ANIO                =2014
- AND   RFPE_NUM_CONTRATO        = RFAC_NUM_CONTRATO
- AND   RFAC_NUM_CONTRATO IN    (1045815,460857,1005127,1013494,101475,1045815)
- AND   RFDO_ANIO_MES            = RFAC_ANIO_MES
- AND   RFDO_NUM_CONTRATO        = RFAC_NUM_CONTRATO
- AND   RFDO_NUM_PERSONA         = RFAC_NUM_PERSONA_PRIMER_TIT
- AND   RFDO_CODX_TIPO_DOMICILIO = "POST"
- AND   COPO_CODIGO_POSTAL       = RFDO_CODX_POSTAL
- AND   (
-        (RFPE_CVE_TIPO_PERSONA IN ("FISI","FACT")  AND
-         RFPE_CVE_ROL_CONTRATO IN ("PTIT", "AEHI", "COTI"))   OR
-        (RFPE_CVE_TIPO_PERSONA = "MORA"  AND
-         RFPE_CVE_ROL_CONTRATO IN ("PTIT","AEHI","FIDU","FIHI",
-                                   "ASEX","FUMA","FMHI","INEX"))
-       )
- FOR BROWSE ACCESS;

- EXIT;
