            :TRAN-NUM-CONTRATO          ,
            :TRAN-NUM-PRODUCTO          ,
            :TRAN-CVE-OPER              ,
            :ORMD-FECH-LIQUIDACION      ,
            :ORMD-TASA-PACTADA          ,
            :TRAN-IMPO-TRANSACCION      ,
            :TRMD-CVE-TIPO-SALDO        ,
            :TRMD-CODX-TIPO-CLIENTE     ,
            :TRMD-TIT-TRANSACCION       ,
            :TRMD-PRED-TRANSACCION      ,
            :TRMD-PLZO-DIAS             ,
            :TRMD-TASA-DESCUENTO        ,
            :TRMD-IMPO-PREMIO           ,
            :TRMD-IMPO-NETO             ,
            :TRMD-CVE-TIPO-ORDEN        ,
            :TRMD-CODX-STATUS-TIPO-ORDEN,
            :CVME-CODX-MOV-TITULO       ,
            :EMIS-CVE-TIPO-VALOR        ,
            :EMIS-DESC-VALOR            ,
            :EMIS-CVE-SERIE-VALOR       ,
            :EMIS-FECH-EMISION          ,
            :EMIS-FECH-VENCIMIENTO-RF   ,
            :EMIS-PREG-NOMINAL          ,
            :EMIS-NUM-CUPON-PAGO-RV     ,
